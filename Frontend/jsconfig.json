{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@app/*": [
                "src/app/*"
            ],
            "@components/*": [
                "src/Components/*"
            ],
            "@hooks/*": [
                "src/Hooks/*"
            ],
            "@lib/*": [
                "src/Lib/*"
            ],
            "@styles/*": [
                "src/Styles/*"
            ],
            "@utils/*": [
                "src/Utils/*"
            ],
            "@assets/*": [
                "src/Assets/*"
            ],
            "@client/*": [
                "src/Clients/*"
            ],
            "@pages/*": [
                "src/Pages/*"
            ],
            "@service/*": [
                "src/Service/*"
            ],
        }
    },
    "include": [
        "src",
        "jsx",
        "js"
    ]
}